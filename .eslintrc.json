{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "html"
    ],
  "rules": {
    "max-len": "off",
    "spaced-comment": "off",
    "new-cap": "off",
    "require-jsdoc": "off",
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": "error",
    "no-array-constructor": "off",
    "@typescript-eslint/no-array-constructor": ["error"],
    "valid-jsdoc": "off"
  }
}
